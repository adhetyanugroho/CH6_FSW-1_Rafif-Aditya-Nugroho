{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "semi": "error",
    "indent": ["error", 2],
    "quotes": ["error", "single"],
    "no-unused-vars": "warn",
    "no-undef": "error",
    "no-const-assign": "error",
    "no-param-reassign": "error",
    "no-multiple-empty-lines": ["error", { "max": 2 }],
    "import/no-unresolved": "off"
    }
}
